# Local development environment variables for Ledger service
# Copy this file to .env.local and update values

# Database (optional - defaults to postgresql://localhost:5432/ledger_development)
# DATABASE_URL=postgresql://localhost:5432/ledger_development

# gRPC server port (optional - defaults to 9091)
# GRPC_PORT=9091

# NATS connection (optional - defaults to nats://localhost:4222)
# NATS_URL=nats://localhost:4222
# NATS_STREAM=rails_events

# Rails environment
RAILS_ENV=development

# Logging
LOG_LEVEL=debug
